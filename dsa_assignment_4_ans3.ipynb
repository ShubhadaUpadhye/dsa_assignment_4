   "source": [
    "#ans 3)\n",
    "a=[[1,2,3],[4,5,6],[7,8,9]]\n",
    "n=len(a)\n",
    "k=0\n",
    "j=0\n",
    "while k<n-1:\n",
    "    while j<n:\n",
    "        a[j][k],a[k][j]=a[k][j],a[j][k]\n",
    "        j=j+1\n",
    "    k=k+1\n",
    "    j=n-k\n",
    "print(a)\n",
    "\n",
    "# time_complexity is O(n^2)"
   ]
  }
