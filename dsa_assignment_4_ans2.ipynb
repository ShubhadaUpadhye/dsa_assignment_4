    "#ans 2)\n",
    "num1 = [1,2,3]\n",
    "num2 = [2,4,6]\n",
    "num3=[[],[]]\n",
    "\n",
    "for i in num1:\n",
    "    if num1.count(i)==1 and i not in num2:\n",
    "        num3[0].append(i)\n",
    "for j in num2:\n",
    "    if num2.count(j)==1 and j not in num1:\n",
    "        num3[1].append(j)\n",
    "print(num3)\n",
    "\n",
    "## time_complexity is O(n)"
   ]
  }
