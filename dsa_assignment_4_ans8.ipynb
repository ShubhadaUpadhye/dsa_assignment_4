    "# ans_8)\n",
    "nums = [2,5,1,3,4,7]\n",
    "nums1=[]\n",
    "n=3\n",
    "for i in range(n,len(nums)):\n",
    "    nums1.append(nums[n])\n",
    "    nums.remove(nums[n])\n",
    "i=1\n",
    "while i<(2*n):\n",
    "    nums.insert(i,nums1[0])\n",
    "    nums1.remove(nums1[0])\n",
    "    i=i+2\n",
    "print(nums)\n",
    "\n",
    "#time_complexity is O(n)"
   ]
  }
